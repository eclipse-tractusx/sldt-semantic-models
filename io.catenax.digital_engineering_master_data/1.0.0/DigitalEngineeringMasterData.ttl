   samm:dataType xsd:float ;
   samm-c:unit samm-u:kilogram .

:MomentOfInertiaCharacteristic a samm-c:Measurement ;
   samm:preferredName "Moment Of Inertia Characteristic"@en ;
   samm:description "Resistance of the part to rotational acceleration around an axis\n kg·m²"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:kilogramPerSquareMetre .

:SurfaceAreaCharacteristic a samm-c:Measurement ;
   samm:preferredName "Surface Area Characteristic"@en ;
   samm:description "surface area in mm2"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:squareMillimetre .

:VolumeCharacteristic a samm-c:Measurement ;
   samm:preferredName "Volume Characteristic"@en ;
   samm:description "volume of the part in mm3"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:cubicMillimetre .

:WeightedWeightCharacteristic a samm-c:Measurement ;
   samm:preferredName "Weighted Weight Characteristic"@en ;
   samm:description "Mass of the part, typically in kilograms. Calculated out of all belonging 3D-models (== Actual weight)"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:kilogram .

:perKelvin a samm:Unit ;
   samm:preferredName "per kelvin"@en ;
   samm:description "Unit for the Thermal Expansion Coefficient, the rate of material expansion per degree of temperature change."@en ;
