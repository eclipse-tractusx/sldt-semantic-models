:BoundingBoxCharacteristic a samm:Characteristic ;
   samm:preferredName "Bounding Box Characteristic"@en ;
   samm:description "The minimal 3D box that fully encloses the part geometry (length x width x height), units in millimetre."@en ;
   samm:dataType :BoundingBoxEntity .

:BoundingBoxEntity a samm:Entity ;
   samm:preferredName "Bounding Box Entity"@en ;
   samm:description "Entity for valuues of the 3D box that fully encloses the part geometry (length x width x height), units in millimetre"@en ;
   samm:properties ( :boundingBoxPropertyLenght :boundingBoxPropertyWidth :boundingBoxPropertyHeight ) .

:BoundingSphereCharacteristic a samm-c:Measurement ;
   samm:preferredName "Bounding Sphere Characteristic"@en ;
   samm:description "Diameter of the smallest possible sphere that fully encloses the part geometry. { \"value\": 315.0, \"unit\": \"mm\" }"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:millimetre .

:CalculatedWeightCharacteristic a samm-c:Measurement ;
   samm:preferredName "Calculated Weight Characteristic"@en ;
   samm:description "Mass of the part, typically in kilograms. Calculated out of all belonging 3D-models"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:kilogram .

:WeightToleranceTrait a samm-c:Trait ;
   samm:preferredName "Weight Tolerance Trait"@en ;
