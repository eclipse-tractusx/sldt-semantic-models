@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.pcf_for_battery_component_supplier:1.0.0#> .
@prefix ext-bpn: <urn:samm:io.catenax.shared.business_partner_number:2.0.0#> .
@prefix ext-pcf: <urn:samm:io.catenax.pcf:9.0.0#> .
@prefix ext-ic: <urn:samm:io.catenax.shared.industry_core.common:1.0.0#> .

:PcfForBatteryComponentSupplier a samm:Aspect ;
   samm:preferredName "PCF for battery component supplier"@en ;
   samm:description "Intermediate Datamodel to enable Supplier to provide PCF information for their products according to the preliminary Delegated Act. This model is to be replaced as soon as the Delegated Act is finalized"@en ;
   samm:properties ( :pcfValues ext-pcf:secondaryEmissionFactorSources [ samm:property ext-pcf:primaryDataShare; samm:optional true ] ext-ic:customerPartId :technologicalRepresentativeness :geographicalRepresentativeness :timerelatedRepresentativeness :dataQualityRating ext-ic:manufacturerPartId ext-ic:validityPeriod :siteIdentifier :legalidentifier :productQualityParameter :contentC02Metals ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:pcfValues a samm:Property ;
   samm:preferredName "product carbon footprint values"@en ;
   samm:description "This attribute contains the values to calculated the PCF values."@en ;
   samm:characteristic :CharacteristicPCFValues .

:technologicalRepresentativeness a samm:Property ;
   samm:preferredName "technological representativeness"@en ;
   samm:description "Data Quality Rating for technological context. Value between 1-5."@en ;
   samm:characteristic ext-pcf:DqiNumberTrait .

:geographicalRepresentativeness a samm:Property ;
   samm:preferredName "geographical representativeness"@en ;
   samm:description "Quality Rating for Data in context of georaphical product information."@en ;
   samm:characteristic ext-pcf:DqiNumberTrait .

:timerelatedRepresentativeness a samm:Property ;
   samm:preferredName "timerelated representativeness"@en ;
   samm:description "Data quality rating for data in context of time."@en ;
   samm:characteristic ext-pcf:DqiNumberTrait .

:dataQualityRating a samm:Property ;
   samm:preferredName "data quality rating"@en ;
   samm:description "Data quality Rating measurement. Value between 1-5. Depending on TimeRepresentatives[TIR], GeoraphicalRepresentatives[GER] and TimeRepresenatives[TIR]."@en ;
   samm:characteristic ext-pcf:DqiNumberTrait .

:siteIdentifier a samm:Property ;
   samm:preferredName "business partner number site (BPN-S)"@en ;
   samm:description "Site identifier of the providing Catena-X participant"@en ;
   samm:characteristic ext-bpn:BpnsTrait .

:legalidentifier a samm:Property ;
   samm:preferredName "business partner number legal (BPN-L)"@en ;
   samm:description "Legal identifier of the providing Catena-X participant."@en ;
   samm:characteristic ext-bpn:BpnlTrait .

:productQualityParameter a samm:Property ;
   samm:preferredName "product quality parameter"@en ;
   samm:description "Quality parameters of the product."@en ;
   samm:characteristic :StringArray ;
   samm:exampleValue "purity of active material: 99.8%; specific capacity of NMC811: 200 mAh/g; energy density of cells: 250 Wh/kg" .

:contentC02Metals a samm:Property ;
   samm:preferredName "content co2 metals"@en ;
   samm:description "C02 relevant metaly contained in the product."@en ;
   samm:characteristic :StringArray ;
   samm:exampleValue "Material: Nickel, Specific Content: 50.5%, EV_Mat: 4.9 kg CO₂e/kg ,Erecycled_Mat: 2.3 kg CO₂e/kg; Material: Cobalt, Specific Content: 6.0%, EV_Mat: 13.0 kg CO₂e/kg ,Erecycled_Mat: 4.2 kg CO₂e/kg" .

:CharacteristicPCFValues a samm:Characteristic ;
   samm:preferredName "characteristic PCF valies"@en ;
   samm:description "characteristic for the entity which contains the pcf information."@en ;
   samm:dataType :EntityPCF .

:StringArray a samm-c:List ;
   samm:preferredName "string array"@en ;
   samm:description "this is a list of strings."@en ;
   samm:dataType xsd:string .

:EntityPCF a samm:Entity ;
   samm:preferredName "entity pcf"@en ;
   samm:description "entity which contains the the pcf information."@en ;
   samm:properties ( :sharesPerStage [ samm:property :pcfPerformanceClass; samm:optional true ] [ samm:property :webLinkToCarbonFootPrintStudy; samm:optional true ] ) .

:sharesPerStage a samm:Property ;
   samm:preferredName "shares per stage"@en ;
   samm:description "encapsulation for pcf values per stage."@en ;
   samm:characteristic :CharacteristicStagePercentages .

:pcfPerformanceClass a samm:Property ;
   samm:preferredName "PCF performance class"@en ;
   samm:description "pcf performance class."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "B+" .

:webLinkToCarbonFootPrintStudy a samm:Property ;
   samm:preferredName "link to carbon footprint study"@en ;
   samm:description "Link to used Carbon Foot Print Study."@en ;
   samm:characteristic samm-c:ResourcePath ;
   samm:exampleValue "https://www.example.com/study.pdf"^^xsd:anyURI .

:CharacteristicStagePercentages a samm:Characteristic ;
   samm:preferredName "characteristic stage percentages"@en ;
   samm:description "Characteristic to describe a pcf stage."@en ;
   samm:dataType :EntityStagePcfValue .

:EntityStagePcfValue a samm:Entity ;
   samm:preferredName "entity stage PCF value"@en ;
   samm:description "Entity which contains the absolute pcf value, value per stage and declared Unit."@en ;
   samm:properties ( :stage1RawMaterialAquisitionPreProcessing :stage2MainProductProduction :stage3Distribution :stage4EndOfLifeRecycling :declaredUnit :absPcfValue ) .

:stage1RawMaterialAquisitionPreProcessing a samm:Property ;
   samm:preferredName "raw material acquisition and pre-processing"@en ;
   samm:description "This attribute describes the percentage of the absolute PCF value of this component that is attributable to the 'Raw material and preprocessing' stage."@en ;
   samm:characteristic :CharacteristicStage .

:stage2MainProductProduction a samm:Property ;
   samm:preferredName "main product production"@en ;
   samm:description "This attribute describes the percentage of the absolute PCF value of this component that is attributable to the 'Raw material and preprocessing' stage."@en ;
   samm:characteristic :CharacteristicStage .

:stage3Distribution a samm:Property ;
   samm:preferredName "stage distribution"@en ;
   samm:description "This attribute describes the percentage of the absolute PCF value of this component that is attributable to the 'Raw material and preprocessing' stage."@en ;
   samm:characteristic :CharacteristicStage .

:stage4EndOfLifeRecycling a samm:Property ;
   samm:preferredName "End-Of-Life and recycling"@en ;
   samm:description "This attribute describes the percentage of the absolute PCF value of this component that is attributable to the 'Raw material and preprocessing' stage."@en ;
   samm:characteristic :CharacteristicStage .

:declaredUnit a samm:Property ;
   samm:preferredName "declared unit"@en ;
   samm:description "This attribute defines the unit of the attribute absPCFValue. It is one of C02KG/kg OR C02KG/piece\"."@en ;
   samm:characteristic :CharacteristicDeclaredUnit .

:absPcfValue a samm:Property ;
   samm:preferredName "abs PCF"@en ;
   samm:description "Absolute Product Carbon Value."@en ;
   samm:characteristic :CharacteristicStage .

:CharacteristicStage a samm:Characteristic ;
   samm:preferredName "characteristic stage"@en ;
   samm:description "Describes an entry for a pcf-life-cycle stage."@en ;
   samm:dataType :EntityStageValue .

:CharacteristicDeclaredUnit a samm-c:Enumeration ;
   samm:preferredName "characteristic declared unit"@en ;
   samm:description "characteristic for declared Unit property."@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "C02KG/KG" "C02KG/piece" ) .

:EntityStageValue a samm:Entity ;
   samm:preferredName "entity stage value"@en ;
   samm:description "Entity which contains the pcf value of a life-cycle-stage."@en ;
   samm:properties ( :stageValue ) .

:stageValue a samm:Property ;
   samm:preferredName "stage value"@en ;
   samm:description "Enter the PCF value of the parent stage measured in the declared unit."@en ;
   samm:characteristic :StageValueTrait ;
   samm:exampleValue "3.14"^^xsd:float .

:StageValueTrait a samm-c:Trait ;
   samm:preferredName "stage value trait"@en ;
   samm:description "Trait to assure the characteristics and constraints of stage value."@en ;
   samm-c:baseCharacteristic :CharacteristicStageValue ;
   samm-c:constraint :ConstraintGEQ .

:CharacteristicStageValue a samm:Characteristic ;
   samm:preferredName "characteristic stage value"@en ;
   samm:description "Characteristic for each stage value."@en ;
   samm:dataType xsd:float .

:ConstraintGEQ a samm-c:RangeConstraint ;
   samm:preferredName "constraint greater or equal"@en ;
   samm:description "greater or equal constraint."@en ;
   samm-c:minValue "0"^^xsd:float ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:LESS_THAN .
