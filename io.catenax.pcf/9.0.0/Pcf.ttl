   samm-c:values ( "Africa" "Americas" "Asia" "Europe" "Oceania" "Australia and New Zealand" "Central Asia" "Eastern Asia" "Eastern Europe" "Latin America and the Caribbean" "Melanesia" "Micronesia" "Northern Africa" "Northern America" "Northern Europe" "Polynesia" "South-eastern Asia" "Southern Asia" "Southern Europe" "Sub-Saharan Africa" "Western Asia" "Western Europe" "Global" "Several" ) .

:FreeAttributionInMassBalancingCharacteristic a samm-c:Enumeration ;
   samm:preferredName "FreeAttributionInMassBalancingCharacteristic"@en ;
   samm:description "Enum values for freeAttributionInMassBalancin"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "true" "false" "not applicable" ) .

:CrossSectoralStandardSet a samm-c:List ;
   samm:preferredName "Cross Sectoral Standards Used Characteristic"@en ;
   samm:description "Characteristic for defining the list of valid accounting standards used for product carbon footprint calculation as specified in the Catena-X PCF Rulebook in accordance with the technical specifications for PCF Data Exchange (Version 2.0.0) from the WBCSD (World Business Council for Sustainable Development)/ PACT initiative."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:dataType xsd:string .

:ProductOrSectorSpecificRuleSet a samm-c:Set ;
   samm:preferredName "Product Or Sector Specific Rules Set"@en ;
   samm:description "Characteristic for defining the set of product or sector specific rules of a product carbon footprint as specified in the Catena-X PCF Rulebook in accordance with the technical specifications for PCF Data Exchange (Version 2.0.0) from the WBCSD (World Business Council for Sustainable Development)/ PACT initiative."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:dataType xsd:string .

:IpccCharacterizationFactorsCharacteristic a samm-c:Enumeration ;
   samm:preferredName "Ipcc Characterization Factors Characteristic"@en ;
   samm:description "Characteristic for defining the characterization factors of a product carbon footprint as specified in the Catena-X PCF Rulebook in accordance with the technical specifications for PCF Data Exchange (Version 2.0.0) from the WBCSD (World Business Council for Sustainable Development)/ PACT initiative. In Catena-X for example specified by a default value."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:dataType xsd:string ;
   samm-c:values ( "AR5" "AR6" "AR4" "unspecified" ) .

:AllocationWasteIncinerationCharacteristic a samm-c:Enumeration ;
   samm:preferredName "Allocation Waste Incineration Characteristic"@en ;
   samm:description "Characteristic for defining the allocation approach used for waste incineration as specified by the TFS (Together For Sustainability) initiative."@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "cut-off" "reverse cut-off" "system expansion" "polluter pays principle" ) .

:AllocationRecycledCarbonCharacteristic a samm-c:Enumeration ;
   samm:preferredName "Allocation Recycled Carbon Characteristic"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "upstream system expansion" "cut-off" ) .

:PrecedingPfId a samm:Entity ;
   samm:preferredName "Preceding PCF (Product Carbon Footprint) Identifier Entity"@en ;
   samm:description "Entity for defining a preceding PCF (Product Carbon Footprint) identifier entity as specified in the Catena-X PCF Rulebook in accordance with the technical specifications for PCF Data Exchange (Version 2.0.0) from the WBCSD (World Business Council for Sustainable Development)/ PACT initiative."@en ;
