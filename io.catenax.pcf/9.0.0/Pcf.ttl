@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.pcf:9.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#> .

:Pcf a samm:Aspect ;
   samm:preferredName "Product (Carbon) Footprint"@en ;
   samm:description "A Product (Carbon) Footprint represents the carbon footprint of a product with values as specified in the Catena-X PCF Rulebook in accordance with the WBCSD (World Business Council for Sustainable Development) Pathfinder framework and the technical specifications for PCF Data Exchange (Version 2.0.0) from the WBCSD/ PACT initiative."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:properties ( :attestationOfConformance :scopeOfPcfForm :companyAndProductInformation :pcfAssessmentAndMethodology :general :carbonContent :productLifeCycleStagesAndEmissions ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:attestationOfConformance a samm:Property ;
   samm:preferredName "Attestation Of Conformance"@en ;
   samm:description "Connects PCF with AttestationOfConformance to link it with AttestationOfConformanceCollection , establishing the semantic relationship and context between both entities.\nProperty to collect all attestation of conformance properties."@en ;
   samm:characteristic :AttestationOfConformanceCollection .

:scopeOfPcfForm a samm:Property ;
   samm:preferredName "Scope Of PCF Form"@en ;
   samm:description "Connects PCF with ScopeOfPCFForm to link it to ScopeOfPCFFormCollection , establishing the semantic relationship and context between both entities.\nProperty to collect all scope of pcf form properties."@en ;
   samm:characteristic :ScopeOfPCFFormCollection .

:companyAndProductInformation a samm:Property ;
   samm:preferredName "Company And Product Information"@en ;
   samm:description "Connects PCF with CompanyAndProductInformation to link it to CompanyAndProductInformation , establishing the semantic relationship and context between both entities.\nProperty to collect all company and production information properties."@en ;
   samm:characteristic :CompanyAndProductInformationCollection .

:pcfAssessmentAndMethodology a samm:Property ;
   samm:preferredName "PCF Assessment And Methodology"@en ;
   samm:description "Connects PCF with haspcfAssessmentAndMethodology to link it to pcfAssessmentAndMethodologyCollection , establishing the semantic relationship and context between both entities.\nProperty to collect all pcf assessment and methodology properties."@en ;
   samm:characteristic :pcfAssessmentAndMethodologyCollection .

:general a samm:Property ;
   samm:preferredName "General"@en ;
   samm:description "Connects PCF with General to link it to GeneralCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all general properties."@en ;
   samm:characteristic :GeneralCollection .

:carbonContent a samm:Property ;
   samm:preferredName "Carbon Content"@en ;
   samm:description "Connects PCF with CarbonContent to link it to CarbonContentCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all carbon content properties."@en ;
   samm:characteristic :CarbonContentCollection .

:productLifeCycleStagesAndEmissions a samm:Property ;
   samm:preferredName "Product Life Cycle Stages And Emissions"@en ;
   samm:description "Connects PCF with ProductLifeCycleStagesAndEmissions to link it to ProductLifeCycleStagesAndEmissionsCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all product life cycle stages and emissions properties."@en ;
   samm:characteristic :ProductLifeCycleStagesAndEmissionsCollection .

:AttestationOfConformanceCollection a samm-c:Collection ;
   samm:preferredName "Attestation Of Conformance Collection"@en ;
   samm:description "Collection of Attestation Of Conformance properties."@en ;
   samm:dataType :AttestationOfConformanceEntity .

:ScopeOfPCFFormCollection a samm-c:Collection ;
   samm:preferredName "Scope Of PCF Form Collection"@en ;
   samm:description "Collection of Scope Of PCF Form properties."@en ;
   samm:dataType :ScopeOfPCFFormEntity .

:CompanyAndProductInformationCollection a samm-c:Collection ;
   samm:preferredName "Company And Product Information Collection"@en ;
   samm:description "Collection of Company And Product Information properties."@en ;
   samm:dataType :CompanyAndProductInformationEntity .

:pcfAssessmentAndMethodologyCollection a samm-c:Collection ;
   samm:preferredName "PCF Assessment and Methodology Collection"@en ;
   samm:description "Collection of PCF Assessment and Methodology properties."@en ;
   samm:dataType :pcfAssessmentAndMethodologyEntity .

:GeneralCollection a samm-c:Collection ;
   samm:preferredName "General Collection"@en ;
   samm:description "Collection of General properties."@en ;
   samm:dataType :GeneralEntity .

:CarbonContentCollection a samm-c:Collection ;
   samm:preferredName "Carbon Content Collection"@en ;
   samm:description "Collection of Carbon Content properties."@en ;
   samm:dataType :CarbonContentEntity .

:ProductLifeCycleStagesAndEmissionsCollection a samm-c:Collection ;
   samm:preferredName "Product Life Cycle Stages And Emissions Collection"@en ;
   samm:description "Collection of Product Life Cycle Stages And Emissions properties."@en ;
   samm:dataType :ProductLifeCycleStagesAndEmissionsEntity .

:AttestationOfConformanceEntity a samm:Entity ;
   samm:preferredName "Attestation Of Conformance Entity"@en ;
   samm:description "Entity to group all attestation of conformance properties."@en ;
   samm:properties ( :attestationType :standardName :attestationStandard :attestationOfConformanceId :providerName [ samm:property :attestationOfConformanceLink; samm:optional true ] [ samm:property :providerId; samm:optional true ] [ samm:property :completedAt; samm:optional true ] ) .

:ScopeOfPCFFormEntity a samm:Entity ;
   samm:preferredName "Scope Of PCF Form Entity"@en ;
   samm:description "Entity to group all scope of pcf form properties."@en ;
   samm:properties ( :specVersion :partialFullPcf ) .

:CompanyAndProductInformationEntity a samm:Entity ;
   samm:preferredName "Company And Product Information Entity"@en ;
   samm:description "Entity to group all company and product information properties."@en ;
   samm:properties ( [ samm:property :companyInformation; samm:optional true ] [ samm:property :productInformation; samm:optional true ] ) .

:pcfAssessmentAndMethodologyEntity a samm:Entity ;
   samm:preferredName "PCF Assessment And Methodology Entity"@en ;
   samm:description "Entity to group all pcf assessment and methodology properties."@en ;
   samm:properties ( [ samm:property :pcfAssessmentInformation; samm:optional true ] [ samm:property :dataSourcesAndQuality; samm:optional true ] [ samm:property :verificationAndCertificationShares; samm:optional true ] [ samm:property :pcfMethodology; samm:optional true ] ) .

:GeneralEntity a samm:Entity ;
   samm:preferredName "General Entity"@en ;
   samm:description "Entity to group all general properties."@en ;
   samm:properties ( :comment :pcfLegalStatement ) .

:CarbonContentEntity a samm:Entity ;
   samm:preferredName "Carbon Content Entity"@en ;
   samm:description "Entity to group all carbon content properties."@en ;
   samm:properties ( :carbonContentTotal :fossilCarbonContent :biogenicCarbonContent :packagingBiogenicCarbonContent :recycledCarbonContent ) .

:ProductLifeCycleStagesAndEmissionsEntity a samm:Entity ;
   samm:preferredName "Product Life Cycle Stages And Emissions Entity"@en ;
   samm:description "Entity to group all product life cycle stages and emissions properties."@en ;
   samm:properties ( :productionStage :distributionStage :packagingStage ) .

:attestationType a samm:Property ;
   samm:preferredName "Attestation type"@en ;
   samm:description "Attestation type, which defines the type and level of trust conveyed by this attestation of conformance (PCF Program certification; PCF 3rd party verification ; PCF 2nd party verification; PCF 1st party verification; ...)"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "PCF Program Certification" .

:standardName a samm:Property ;
   samm:preferredName "Conformant with cross-sectoral standards or product or sector rules"@en ;
   samm:description "The specific cross-sectoral standards or product or sector rules (PCRs) on which the attestation of conformance is based"@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Catena-X Product Carbon Footprint Rulebook v4" .

:attestationStandard a samm:Property ;
   samm:preferredName "Attestation scheme standard "@en ;
   samm:description "e.g. PCF Verification and PCF Program Certification Framework"@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "PCF Verification and PCF Program Certification Framework V2" .

:attestationOfConformanceId a samm:Property ;
   samm:preferredName "Attestation ID"@en ;
   samm:description "e.g. unique number of the certificate or verification statement used for tracking and referencing, use UUID v4 if ID is newly generated."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "4a7c4482-8431-4c31-a895-70341d2a1376" .

:providerName a samm:Property ;
   samm:preferredName "Issuer of attestation"@en ;
   samm:description "Name of the issuing certifier’s or verifier’s legal entity"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "TÜV-X" .

:attestationOfConformanceLink a samm:Property ;
   samm:preferredName "Link to attestation"@en ;
   samm:description "A link leading to the declaration of conformance, allowing for a manual option to verify the validity and authenticity of the declaration."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "www.Certifierrepository_example.com/Certificate 123456" .

:providerId a samm:Property ;
   samm:preferredName "Issuer of attestation ID"@en ;
   samm:description "A unique identifier for the entity issuing the declaration, such as a Business Partner Number (BPN) or other official registration number, issued by the appointing organization or accreditation institute."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "urn:cofinity-x.com:bpn:BPNL0000000AXXXX" .

:completedAt a samm:Property ;
   samm:preferredName "Date of attestation"@en ;
   samm:description "Time stamp for when the attestation of conformity was issued"@en ;
   samm:characteristic samm-c:Timestamp ;
   samm:exampleValue "2022-12-31T23:59:59Z"^^xsd:dateTime .

:specVersion a samm:Property ;
   samm:preferredName "Product Footprint Specification Version"@en ;
   samm:description "Specification of the PCF format/data model (name and version number), which is used. The required data input fields will be tailored accordingly. Multiple entries are possible. The data model and version can be selected independently of the standard or guidance document, you followed during the assessment of the PCF. Always select latest data model, if your data supports it."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :SpecVersionCharacteristic ;
   samm:exampleValue "urn:io.catenax.pcf:datamodel:version:8.0.0" .

:partialFullPcf a samm:Property ;
   samm:preferredName "Partial Or Full PCF (Product Carbon Footprint)"@en ;
   samm:description "This attribute is an indicator for partial or full PCF (Product Carbon Footprint) declaration. A partial PCF (cradle-to-gate) is covering the emissions from resource extraction until the product leaves the gate of your organization (optionally including the distribution stage). This is defaulted for PACT, Catena-X or TFS. A full PCF (cradle-to-grave) is covering the complete life cycle of the product from resource extraction all the way to end-of-life stage."@en ;
   samm:characteristic :PartialFullPcfCharacteristic ;
   samm:exampleValue "Cradle-to-gate" .

:companyInformation a samm:Property ;
   samm:preferredName "Company Information"@en ;
   samm:description "Connects CompanyAndProductInformationEntity with ProductInformation to link it with ProductInformationCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all company information properties."@en ;
   samm:characteristic :CompanyInformationCollection .

:productInformation a samm:Property ;
   samm:preferredName "Product Information"@en ;
   samm:description "Connects CompanyAndProductInformationEntity with ProductInformation to link it with ProductInformationCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all product information properties."@en ;
   samm:characteristic :ProductInformationCollection .

:pcfAssessmentInformation a samm:Property ;
   samm:preferredName "PCF Assessment Information"@en ;
   samm:description "Connects pcfAssessmentAndMethodologyEntity with PCFAssessmentInformation to link it with PCFAssessmentInformationCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all pcf assessment information properties."@en ;
   samm:characteristic :PCFAssessmentInformationCollection .

:dataSourcesAndQuality a samm:Property ;
   samm:preferredName "Data Sources And Quality"@en ;
   samm:description "Connects pcfAssessmentAndMethodologyEntity with DataSourcesAndQuality to link it with DataSourcesAndQualityCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all data sources and quality properties."@en ;
   samm:characteristic :DataSourcesAndQualityCollection .

:verificationAndCertificationShares a samm:Property ;
   samm:preferredName "Verification And Certification Shares"@en ;
   samm:description "Connects pcfAssessmentAndMethodologyEntity with VerificationAndCertificationShares to link it with VerificationAndCertificationSharesCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all verification and certification shares properties."@en ;
   samm:characteristic :VerificationAndCertificationSharesCollection .

:pcfMethodology a samm:Property ;
   samm:preferredName "PCF Methodology"@en ;
   samm:description "Connects pcfAssessmentAndMethodologyEntity with PCFMethodology to link it with PCFMethodologyCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all pcf methodology properties."@en ;
   samm:characteristic :PCFMethodologyCollection .

:comment a samm:Property ;
   samm:preferredName "Comment"@en ;
   samm:description "This text field can be used to provide the recipient of the data with additional information or instructions on the calculation that is not represented by the existing attributes of the data model."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Additional explanatory information not reflected by other attributes" .

:pcfLegalStatement a samm:Property ;
   samm:preferredName "PCF (Product Carbon Footprint) Legal Statement"@en ;
   samm:description "In case your organization defined certain legal conditions which apply to the publication of this PCF, you can state the legal disclaimer in here. The text might include an URL link to the legal disclaimer of the PCF provider. For Catena-X this only refers to legal requirements, not negociated automatically among the EDCs."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "This PCF (Product Carbon Footprint) is for information purposes only. It is based upon the standards mentioned above." .

:carbonContentTotal a samm:Property ;
   samm:preferredName "Total carbon content"@en ;
   samm:description "Total carbon content"@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.52"^^xsd:decimal .

:fossilCarbonContent a samm:Property ;
   samm:preferredName "Fossil Carbon Content"@en ;
   samm:description "Carbon content defined as fossil."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20231207/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.1"^^xsd:decimal .

:biogenicCarbonContent a samm:Property ;
   samm:preferredName "Biogenic Carbon Content"@en ;
   samm:description "Carbon content defined as biogenic. It can be attributed by means of mass balancing."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20231207/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.0"^^xsd:decimal .

:packagingBiogenicCarbonContent a samm:Property ;
   samm:preferredName "Packaging biogenic carbon content"@en ;
   samm:description "Detail (if packaging included) and only optional. The biogenic carbon in the packaging should always be inlcuded in the biogenic carbon content per DU."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20231207/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:recycledCarbonContent a samm:Property ;
   samm:preferredName "Recycled carbon content"@en ;
   samm:description "Carbon content defined as recycled. It can be attributed by means of mass balancing: in such a case the mass balancing used must be set to TRUE. "@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20231207/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:productionStage a samm:Property ;
   samm:preferredName "Production Stage"@en ;
   samm:description "Connects ProductLifeCycleStagesAndEmissionsEntity with ProductionStage to link it with ProductionStageCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all production stage properties."@en ;
   samm:characteristic :ProductionStageCollection .

:distributionStage a samm:Property ;
   samm:preferredName "Distribution Stage"@en ;
   samm:description "Connects ProductLifeCycleStagesAndEmissionsEntity with DistributionStage to link it with DistributionStageCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all distribution stage properties."@en ;
   samm:characteristic :DistributionStageCollection .

:packagingStage a samm:Property ;
   samm:preferredName "Packaging Stage"@en ;
   samm:description "Connects ProductLifeCycleStagesAndEmissionsEntity with PackagingStage to link it with PackagingStageCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all packaging stage properties."@en ;
   samm:characteristic :PackagingStageCollection .

:SpecVersionCharacteristic a samm-c:Set ;
   samm:dataType xsd:string .

:PartialFullPcfCharacteristic a samm-c:Enumeration ;
   samm:preferredName "Partial Or Full PCF (Product Carbon Footprint) Characteristic"@en ;
   samm:description "Characteristic for defining an indicator for partial or full PCF (Product Carbon Footprint) declaration as specified in the Catena-X PCF Rulebook."@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "Cradle-to-gate" "Cradle-to-grave" ) .

:CompanyInformationCollection a samm-c:Collection ;
   samm:preferredName "Company Information Collection"@en ;
   samm:description "Collection of Company Information properties."@en ;
   samm:dataType :CompanyInformationEntity .

:ProductInformationCollection a samm-c:Collection ;
   samm:preferredName "Product Information Collection"@en ;
   samm:description "Collection of Product Information properties."@en ;
   samm:dataType :ProductInformationEntity .

:PCFAssessmentInformationCollection a samm-c:Collection ;
   samm:preferredName "PCF Assessment Information Collection"@en ;
   samm:description "Collection of PCF Assessment Information properties."@en ;
   samm:dataType :PCFAssessmentInformationEntity .

:DataSourcesAndQualityCollection a samm-c:Collection ;
   samm:preferredName "Data Sources And Quality Collection"@en ;
   samm:description "Collection of Data Sources And Quality properties."@en ;
   samm:dataType :DataSourcesAndQualityEntity .

:VerificationAndCertificationSharesCollection a samm-c:Collection ;
   samm:preferredName "Verification And Certification Shares Collection"@en ;
   samm:description "Collection of Verification And Certification Shares properties."@en ;
   samm:dataType :VerificationAndCertificationSharesEntity .

:PCFMethodologyCollection a samm-c:Collection ;
   samm:preferredName "PCF Methodology Collection"@en ;
   samm:description "Collection of PCF Methodology properties."@en ;
   samm:dataType :PCFMethodologyEntity .

:PositiveEmissionsTrait a samm-c:Trait ;
   samm:preferredName "Positive Emissions Trait"@en ;
   samm:description "Trait for emissions equal to or greater than zero in context of a PCF (Product Carbon Footprint)."@en ;
   samm-c:baseCharacteristic :PositiveEmission ;
   samm-c:constraint :PositiveEmissionConstraint .

:ProductionStageCollection a samm-c:Collection ;
   samm:preferredName "Production Stage Collection"@en ;
   samm:description "Collection of Production Stage properties."@en ;
   samm:dataType :ProductionStageEntity .

:DistributionStageCollection a samm-c:Collection ;
   samm:preferredName "Distribution Stage Collection"@en ;
   samm:description "Collection of Distribution Stage properties."@en ;
   samm:dataType :DistributionStageEntity .

:PackagingStageCollection a samm-c:Collection ;
   samm:preferredName "Packaging Stage Collection"@en ;
   samm:description "Collection of Packaging Stage properties."@en ;
   samm:dataType :PackagingStageEntity .

:CompanyInformationEntity a samm:Entity ;
   samm:preferredName "Company Information Entity"@en ;
   samm:description "Entity to group all company information properties."@en ;
   samm:properties ( :companyName :companyIds ) .

:ProductInformationEntity a samm:Entity ;
   samm:preferredName "Product Information Entity"@en ;
   samm:description "Entity to group all product information properties."@en ;
   samm:properties ( :productNameCompany :productIds :productDescription :productClassifications :declaredUnitOfMeasurement :declaredUnitAmount :productMassPerDeclaredUnit ) .

:PCFAssessmentInformationEntity a samm:Entity ;
   samm:preferredName "PCF Assessment Information Entity"@en ;
   samm:description "Entity to group all pcf assessment information properties."@en ;
   samm:properties ( :technology :idAndVersion :boundarySpecifications :geography :time ) .

:DataSourcesAndQualityEntity a samm:Entity ;
   samm:preferredName "Data Sources And Quality Entity"@en ;
   samm:description "Entity to group all data sources and quality properties."@en ;
   samm:properties ( :primaryDataShare :secondaryEmissionFactorSources :technologicalDQR :temporalDQR :geographicalDQR ) .

:VerificationAndCertificationSharesEntity a samm:Entity ;
   samm:preferredName "Verification And Certification Shares Entity"@en ;
   samm:description "Entity to group all verification and certification shares properties."@en ;
   samm:properties ( :programCertificationShare :productVerificationShare3rdParty :productVerificationShare2ndParty :productVerificationShare1stParty ) .

:PCFMethodologyEntity a samm:Entity ;
   samm:preferredName "PCF Methodology Entity"@en ;
   samm:description "Entity to group all pcf methodology properties."@en ;
   samm:properties ( :massBalancingInformation :standards :gwpCharacterizationFactorDetails :allocationInForeground ) .

:PositiveEmission a samm-c:Quantifiable ;
   samm:preferredName "Positive Emission"@en ;
   samm:description "Characteristic for defining emissions expressed as a decimal equal to or greater than zero in context of a PCF (Product Carbon Footprint) as specified in the Catena-X PCF Rulebook in accordance with the technical specifications for PCF Data Exchange (Version 2.0.0) from the WBCSD (World Business Council for Sustainable Development)/ PACT initiative."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:dataType xsd:decimal .

:PositiveEmissionConstraint a samm-c:RangeConstraint ;
   samm:preferredName "PositiveEmissionConstraint"@en ;
   samm:description "Only positive emission values (>=0) are valid "@en ;
   samm-c:minValue "0.0"^^xsd:decimal ;
   samm-c:lowerBoundDefinition samm-c:AT_LEAST ;
   samm-c:upperBoundDefinition samm-c:LESS_THAN .

:ProductionStageEntity a samm:Entity ;
   samm:preferredName "Production Stage Entity"@en ;
   samm:description "Entity to group all production stage properties."@en ;
   samm:properties ( :pcfIncludingBiogenicUptake :pcfExcludingBiogenicUptake :fossilGhgEmissions :biogenicNonCO2Emissions :biogenicCO2Uptake :landUseChangeGhgEmissions :packagingLandManagementBiogenicCO2Emissions :landManagementBiogenicCO2Removals :aircraftGhgEmissions ) .

:DistributionStageEntity a samm:Entity ;
   samm:preferredName "Distribution Stage Entity"@en ;
   samm:description "Entity to group all distribution stage properties."@en ;
   samm:properties ( :distributionStageIncluded :distributionStagePcfIncludingBiogenicUptake :distributionStagePcfExcludingBiogenicUptake :distributionStageFossilGhgEmissions :distributionStageBiogenicNonCO2Emissions :distributionStageBiogenicCO2Uptake :distributionStageLandUseChangeGhgEmissions :distributionStageLandManagementBiogenicCO2Emissions :distributionStageLandManagementBiogenicCO2Removals :distributionStageAircraftGhgEmissions ) .

:PackagingStageEntity a samm:Entity ;
   samm:preferredName "Packaging Stage Entity"@en ;
   samm:description "Entity to group all packaging stage properties."@en ;
   samm:properties ( :packagingEmissionsIncluded :packagingPcfIncludingBiogenicUptake :packagingPcfExcludingBiogenicUptake :packagingFossilGhgEmissions :packagingBiogenicNonCO2Emissions :packagingBiogenicCO2Uptake :packagingLandUseChangeGhgEmissions :packagingLandManagementBiogenicCO2Emissions :packagingLandManagementBiogenicCO2Removals :packagingAircraftGhgEmissions ) .

:companyName a samm:Property ;
   samm:preferredName "Company Name"@en ;
   samm:description "State the (legal) name of the company supplying the product and reporting the PCF (data owner). The name under which the company signed up with Catena-X (via Business Partner Management service)."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :NonEmptyStringTrait ;
   samm:exampleValue "My Corp" .

:companyIds a samm:Property ;
   samm:preferredName "Company IDs"@en ;
   samm:description "Company identifier according to the sharing scheme you are reporting in. It is mandatory to provide the BPNL (Business Partner Number Legal; corresponding to the company name) according to Catena-X. Each value of this set is supposed to uniquely identify the PCF Data Owner and should be in the following format: E.g. urn:BPNL000000000DWF. Additionally it is recommended to enter the name of the main domain (e.g. company.com) as a second value."@en ;
   samm:see <urn:samm:io.catenax.shared.business_partner_number:2.0.0#bpnlProperty> ;
   samm:characteristic :IdSet ;
   samm:exampleValue "urn:BPNL000000000DWF"^^xsd:anyURI .

:productNameCompany a samm:Property ;
   samm:preferredName "Product Trade Name"@en ;
   samm:description "Trade name of the product to make it recognizable by the receiver of the PCF information. For prospective PCFs, this may be a preliminary name."@en ;
   samm:see <urn:samm:io.catenax.part_type_information:1.0.0#nameAtManufacturer> ;
   samm:characteristic :NonEmptyStringTrait ;
   samm:exampleValue "My Product Name" .

:productIds a samm:Property ;
   samm:preferredName "Product IDs"@en ;
   samm:description "A set of several relevant product identifiers can be provided including e.g. manufacturerPartID, customerPartID, GTIN, ISPN-number or any product specific identifier. For prospective PCFs, this may be a preliminary product id."@en ;
   samm:see <urn:samm:io.catenax.part_type_information:1.0.0#manufacturerPartId> ;
   samm:characteristic :IdSet ;
   samm:exampleValue "urn:mycompany.com:product-id:401765"^^xsd:anyURI .

:productDescription a samm:Property ;
   samm:preferredName "Product Description"@en ;
   samm:description "A brief description of the product (for example functions and technical parameters) to help identifying the correct product. Product IDs are not always unique accross organizations or at times insufficient to identify the correct product. This information can provide additional context to secure a correct match between the purches product and its PCF declaration."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Ethanol, 95% solution" .

:productClassifications a samm:Property ;
   samm:preferredName "Product Classification"@en ;
   samm:description "A list of classification or category identifiers in URN format. Use well known urn's here, or adhere to recommended urn:pact: format. For example UN CPC, CAS Number, CN-Code etc. For communication us URN fromat (e.g. urn:gtin:4712345060507)."@en ;
   samm:characteristic :ProductClassificationCharacteristic ;
   samm:exampleValue "urn:gtin:4712345060507" .

:declaredUnitOfMeasurement a samm:Property ;
   samm:preferredName "Unit of measurement"@en ;
   samm:description "The declared unit defines the reference quantity to which the inputs and outputs in a Product Carbon Footprint (PCF) calculation are related, such as kilograms of product, a single piece of a component, or megajoules of electrical energy. Within Catena-X, acceptable values for the declared unit include “piece” and “kilogram,” as outlined in the Catena-X PCF Rulebook v4, chapter 4.4. The PCF is calculated per declared unit amount of the product. For example, a Battery Electric Vehicle (BEV) may be represented in two ways. In the first case, the declared unit amount is 1 and the declared unit is “piece,” resulting in a total PCF including bio uptake of 15,000 kg CO?e. In the second case, the same BEV is described with a declared unit amount of 2,300 and the unit is “kilogram,” while still yielding a total PCF including bio uptake of 15,000 kg CO?e."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :DeclaredUnitOfMeasurementCharacteristic ;
   samm:exampleValue "kilogram" .

:declaredUnitAmount a samm:Property ;
   samm:preferredName "Unitary Product Amount"@en ;
   samm:description "The quantity, or amount, of the declared unit is provided as a numerical value. The Product Carbon Footprint (PCF) is based on this declared unit amount, rather than assuming a default value of one unit. For example, a Battery Electric Vehicle (BEV) may be described with a declared unit amount of 1 and the unit “piece,” resulting in a total PCF including bio uptake of 15,000 kg CO?e. Alternatively, the same BEV might be represented using a declared unit amount of 2,300 with the unit “kilogram,” yet the total PCF including bio uptake remains 15,000 kg CO?e."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :StrictlyPositiveDecimalTrait ;
   samm:exampleValue "1.0"^^xsd:float .

:productMassPerDeclaredUnit a samm:Property ;
   samm:preferredName "Product mass [kg] per declared unit amount"@en ;
   samm:description "The mass of the product per declared unit amount in kilogram, (e.g., the declared unit of a circuitboard is one piece; one piece represents 0.123 kg). Product mass excluding packaging."@en ;
   samm:characteristic :PositiveDecimalWeightTrait ;
   samm:exampleValue "0.456"^^xsd:decimal .

:technology a samm:Property ;
   samm:preferredName "Technology"@en ;
   samm:description "Connects PCFAssessmentInformationEntity with Technology to link it with TechnologyCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all technology properties."@en ;
   samm:characteristic :TechnologyCollection .

:idAndVersion a samm:Property ;
   samm:preferredName "ID And Version"@en ;
   samm:description "Connects PCFAssessmentInformationEntity with IDAndVersion to link it with IDAndVersionCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all id and version properties."@en ;
   samm:characteristic :IDAndVersionCollection .

:boundarySpecifications a samm:Property ;
   samm:preferredName "Boundary Specifications"@en ;
   samm:description "Connects PCFAssessmentInformationEntity with BoundarySpecifications to link it with BoundarySpecificationsCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all boundary specifications properties."@en ;
   samm:characteristic :BoundarySpecificationsCollection .

:geography a samm:Property ;
   samm:preferredName "Geography"@en ;
   samm:description "Connects PCFAssessmentInformationEntity with Geography to link it with GeographyCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all geography properties."@en ;
   samm:characteristic :GeographyCollection .

:time a samm:Property ;
   samm:preferredName "Time"@en ;
   samm:description "Connects PCFAssessmentInformationEntity with Time to link it with TimeCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all time properties."@en ;
   samm:characteristic :TimeCollection .

:primaryDataShare a samm:Property ;
   samm:preferredName "Primary Data Share"@en ;
   samm:description "Share of primary data in the final PCF. For prospective PCF this shall not be communicated or set \"0\""@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :PercentTrait ;
   samm:exampleValue "56.12"^^xsd:decimal .

:secondaryEmissionFactorSources a samm:Property ;
   samm:preferredName "Secondary Emission Factor Sources"@en ;
   samm:description "Which secondary data sources and versions have been used by you or by suppliers (e.g. data bases such as ecoinvent)?"@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :EmissionFactorDSSet ;
   samm:exampleValue "ecoinvent 3.8" .

:technologicalDQR a samm:Property ;
   samm:preferredName "Technological DQR (Data Quality Rating)"@en ;
   samm:description "The degree to which the data reflects the actual technology(ies) used. Refer to Table 5.14 in chapter 5.2.11.2 of v3.0 of TFS Guideline for the rating (1-5). For prospective PCF this shall not be provided."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :DqiNumberTrait ;
   samm:exampleValue "2.0"^^xsd:decimal .

:temporalDQR a samm:Property ;
   samm:preferredName "Temporal DQR (Data Quality Rating)"@en ;
   samm:description "The degree to which the reference period is for the data set is close to the issue date.  Refer to Table 5.16 in chapter 5.2.11.2 of v3.0 of TFS Guideline for the rating (1-5). For prospective PCF this shall not be provided."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :DqiNumberTrait ;
   samm:exampleValue "2.0"^^xsd:decimal .

:geographicalDQR a samm:Property ;
   samm:preferredName "Geographical DQR (Data Quality Rating)"@en ;
   samm:description "Geographical representativeness. The degree to which the data reflects the actual geographic location of the manufacturing process. processes within the inventory boundary (e.g., country or region). Refer to Table 5.15 in chapter 5.2.11.2 of v3.0 of TFS Guideline for the rating (1-5). For prospective PCF this shall not be provided."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :DqiNumberTrait ;
   samm:exampleValue "2.0"^^xsd:decimal .

:programCertificationShare a samm:Property ;
   samm:preferredName "PCS"@en ;
   samm:description "PCF Program Certification Share (PCS) indicates the share of the PCF result which was calculated by PCF program certified suppliers. "@en ;
   samm:characteristic :ProgramCertificationShareTrait .

:productVerificationShare3rdParty a samm:Property ;
   samm:preferredName "3PVS"@en ;
   samm:description "Third-party Product Verification Share (3PVS) represents the share of the PCF result which is based on verified data from an independent third party verifier."@en ;
   samm:characteristic :ProgramCertificationShareTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:productVerificationShare2ndParty a samm:Property ;
   samm:preferredName "2PVS"@en ;
   samm:description "Second-party Product Verification Share (2PVS) represents the share of the PCF result which is based on verified data from a second party verifier."@en ;
   samm:characteristic :ProgramCertificationShareTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:productVerificationShare1stParty a samm:Property ;
   samm:preferredName "1PVS"@en ;
   samm:description "First-party Product Verification Share (1PVS) represents the share of the PCF result which is based on verified data from a first party verifier."@en ;
   samm:characteristic :ProgramCertificationShareTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:massBalancingInformation a samm:Property ;
   samm:preferredName "Mass Balancing Information"@en ;
   samm:description "Connects PCFMethodologyEntity with MassBalancingInformation to link it with MassBalancingInformationCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all mass balancing information properties."@en ;
   samm:characteristic :MassBalancingInformationCollection .

:standards a samm:Property ;
   samm:preferredName "Standards"@en ;
   samm:description "Connects PCFMethodologyEntity with Standards to link it with StandardsCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all standards information properties."@en ;
   samm:characteristic :StandardsCollection .

:gwpCharacterizationFactorDetails a samm:Property ;
   samm:preferredName "GWP Characterization Factor Details"@en ;
   samm:description "Connects PCFMethodologyEntity with GWPCharacterizationFactorDetails to link it with GWPCharacterizationFactorDetailsCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all gwp characterization factor details properties."@en ;
   samm:characteristic :GWPCharacterizationFactorDetailsCollection .

:allocationInForeground a samm:Property ;
   samm:preferredName "Allocation In Foreground"@en ;
   samm:description "Connects PCFMethodologyEntity with AllocationInForeground to link it with AllocationInForegroundCollection, establishing the semantic relationship and context between both entities.\nProperty to collect all allocation in foreground properties."@en ;
   samm:characteristic :AllocationInForegroundCollection .

:pcfIncludingBiogenicUptake a samm:Property ;
   samm:preferredName "GWP total incl. bio. uptake"@en ;
   samm:description "Position T1== A+C+D(negative contribution)+E+F+G(negative contribution)+H. Letters refer to individual emission categories below.\nThis also refers to the  -1/+1 approach.\n\"GWP total inc. bio. uptake\" may be set equal to \"GWP total excl. bio. uptake\" if the product has no or a neglectable biogenic carbon content. General cut-off criteria applies as criteria for neglectability."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :PositiveOrNegativeEmission ;
   samm:exampleValue "1.90"^^xsd:decimal .

:pcfExcludingBiogenicUptake a samm:Property ;
   samm:preferredName "GWP total excl. bio. uptake"@en ;
   samm:description "Position T2=\n= A+C+E+F+G(negative contribution)+H. Letters refer to individual emission categories below.\nThis is also refers to the  0/0 approach."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "2.90"^^xsd:decimal .

:fossilGhgEmissions a samm:Property ;
   samm:preferredName "Fossil Emissions"@en ;
   samm:description "Position A: includes all fossil emissions, including industrial processes, stationary/mobile combustion and fugitive emissions. This position inlcudes the fossil emissions associated to land management (A1: \"GWP fossil land management\") which can not be documented as a separate emission category in the Catena-X data set, but is part of PACT and TFS"@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.5"^^xsd:decimal .

:biogenicNonCO2Emissions a samm:Property ;
   samm:preferredName "GWP biogenic emissions other than CO2"@en ;
   samm:description "Position C: non-CO2 biogenic emissions related to agricultural activities. It encompasses emissions as described in PACT v3.0: CH4 emissions from livestock and manure; CH4 emissions from biomass burning and fires; \nCH4 emissions from rice production; CH4 emissions from transformation and degradation (e.g., combustion, digestion, composting, landfilling). It must be noted that N2O from land management activities are not included in this position and are reported in position A."@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "1.0"^^xsd:decimal .

:biogenicCO2Uptake a samm:Property ;
   samm:preferredName "GWP biogenic CO2-uptake (biogenic CO2 contained in the product)"@en ;
   samm:description "Position D (negative contribution): Biogenic CO2 uptake. The CO2 which was absorbed from the atmosphere during the growth period of the biomass and of which the C is now bound in the product as biogenic carbon content."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20231207/> ;
   samm:characteristic :NegativeEmissionsTrait ;
   samm:exampleValue "-1.00"^^xsd:decimal .

:landUseChangeGhgEmissions a samm:Property ;
   samm:preferredName "GWP land use change (LUC, excluding iLUC)"@en ;
   samm:description "Position E: Emissions from LUC constitute a release of GHG emissions due to a change in land use from one land use category or subcategory to another, such as primary forest to agricultural land, or peat land (type of wetland) to cropland. This position encompasses dLUC (direct land use change) emissions. If that data is not available, companies should account for LUC using statistical land-use change (sLUC) emissions. iLUC emissions are excluded. Refer to PACT v3.0 for details. "@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "1.40"^^xsd:decimal .

:packagingLandManagementBiogenicCO2Emissions a samm:Property ;
   samm:preferredName "Packaging GWP land Management CO2 Emissions"@en ;
   samm:description "Position F: Carbon stock losses occurring within the same land use category or subcategory due to agricultural practices such as tillage, field preparations, pruning and harvest. Land management CO2 emissions measures biogenic CO2 emissions from a net loss in carbon stock within one land use category or subcategory. This includes impact on the land-carbon pools, including above- and below-ground biomass, dead organic matter, and soil carbon pools. If the carbon stock increases within the same land use category and the conditions to report removals are met, this may be calculated as a Land management CO2 removal (position G). Refer to PACT v3.0 for details."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:landManagementBiogenicCO2Removals a samm:Property ;
   samm:preferredName "GWP land Management CO2 Removals"@en ;
   samm:description "Position G (negative contribution): Land management removals are net CO2 removals resulting from net increases to carbon stored in land-based carbon pools (biomass, dead organic matter and soil carbon pools) due to ongoing land management practices. This extra net carbon stock is gained over the crop rotation or crop cultivation cycle (e.g., multiple years for perennial crops and multiple years in a rotation that includes annual crops). Refer to PACT v3.0 for details."@en ;
   samm:characteristic :NegativeEmissionsTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:aircraftGhgEmissions a samm:Property ;
   samm:preferredName "GWP aviation emissions (upstream)"@en ;
   samm:description "Position H: Aviation emissions which have occurred in distribution stages upstream."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.0"^^xsd:decimal .

:distributionStageIncluded a samm:Property ;
   samm:preferredName "Distribution Stage"@en ;
   samm:description "The value \"true\" shall be selected, if emissions related to the outbound transport of your products are included in the system boundary. Mandatory, if transport is paid by reporting company. Distribution stage emissions (outbound) are to be reported separately from the production stage emissions."@en ;
   samm:see <https://wbcsd.github.io/tr/2023/data-exchange-protocol-20230221/> ;
   samm:characteristic samm-c:Boolean .

:distributionStagePcfIncludingBiogenicUptake a samm:Property ;
   samm:preferredName "Distribution stage GWP total incl. bio. uptake"@en ;
   samm:description "Position T1== A+C+D(negative contribution)+E+F+G(negative contribution)+H. Letters refer to individual emission categories below.This also refers to the  -1/+1 Approach.GWP total inc. bio. uptake may be set equal to GWP total excl. bio. uptake if the product has no or a neglectable biogenic carbon content. General Cut-off criteria applies as criteria for neglectability."@en ;
   samm:characteristic :PositiveOrNegativeEmission ;
   samm:exampleValue "0.15"^^xsd:decimal .

:distributionStagePcfExcludingBiogenicUptake a samm:Property ;
   samm:preferredName "Distribution stage  GWP total excl. bio. uptake"@en ;
   samm:description "Position T2== A+C+E+F+G(negative contribution)+H. Letters refer to individual emission categories below.This also refers to the  0/0 approach."@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.15"^^xsd:decimal .

:distributionStageFossilGhgEmissions a samm:Property ;
   samm:preferredName "Distribution Stage Fossil GHG (Greenhouse Gas) Emissions"@en ;
   samm:description "Position A: Includes all fossil emissions, including industrial processes, stationary/mobile combustion and fugitive emissions. This position inlcudes the fossil emissions associated to land management (A1: GWP fossil land management) which can not be documented as a separate emission category in the Catena-X data set, but is part of PACT and TfS. "@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.15"^^xsd:decimal .

:distributionStageBiogenicNonCO2Emissions a samm:Property ;
   samm:preferredName "Distribution stage  GWP biogenic emissions other than CO2"@en ;
   samm:description "Position C: Non-CO2 biogenic emissions related to agricultural activities. It encompasses emissions as described in PACT v3.0: CH4 emissions from livestock and manure; CH4 emissions from biomass burning and fires; CH4 emissions from rice production; CH4 emissions from transformation and degradation (e.g., combustion, digestion, composting, landfilling). It must be noted that N2O from land management activities are not included in this position and are reported in position A."@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:distributionStageBiogenicCO2Uptake a samm:Property ;
   samm:preferredName "Distribution stage  GWP biogenic CO2-uptake (biogenic CO2 contained in the product)"@en ;
   samm:description "Position D (negative contributuion): Biogenic CO2 uptake. The CO2 which was absorbed from the atmosphere during the growth period of the biomass and of which the C is now bound in the product as biogenic carbon content."@en ;
   samm:characteristic :NegativeEmission ;
   samm:exampleValue "0.00"^^xsd:decimal .

:distributionStageLandUseChangeGhgEmissions a samm:Property ;
   samm:preferredName "Distribution stage  GWP land use change (LUC, excluding iLUC)"@en ;
   samm:description "Position E: Emissions from LUC constitute a release of GHG emissions due to a change in land use from one land use category or subcategory to another, such as primary forest to agricultural land, or peat land (type of wetland) to cropland. This position encompasses dLUC (direct land use change) emissions. If that data is not available, companies should account for LUC using statistical land-use change (sLUC) emissions. iLUC emissions are excluded. Refer to PACT v3.0 for details."@en ;
   samm:characteristic :PositiveEmissionsTrait ;
   samm:exampleValue "0.00"^^xsd:decimal .

:distributionStageLandManagementBiogenicCO2Emissions a samm:Property ;
   samm:preferredName "Distribution stage  GWP land Management CO2 Emissions"@en ;
   samm:description "Position F: Carbon stock losses occurring within the same land use category or subcategory due to agricultural practices such as tillage, field preparations, pruning and harvest. Land Management CO2 emissions measures biogenic CO2 emissions from a net loss in carbon stock within one land use category or subcategory. This includes impact on the land-carbon pools, including above- and below-ground biomass, dead organic matter, and soil carbon pools. If the carbon stock increases within the same land use category and the conditions to report removals are met, this may be calculated as a Land management CO2 removal (position G). Refer to PACT v3.0 for details."@en ;
   samm:characteristic :PositiveEmissionsTrait ;
