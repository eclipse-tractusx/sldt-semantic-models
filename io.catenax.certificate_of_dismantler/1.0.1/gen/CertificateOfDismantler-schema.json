        "properties" : {
          "technicalKey" : {
            "description" : "Technical key of a locality",
            "$ref" : "#/components/schemas/urn_bamm_io.catenax.shared.address_characteristic_1.0.1_LocalityTechnicalKeyCharacteristic"
