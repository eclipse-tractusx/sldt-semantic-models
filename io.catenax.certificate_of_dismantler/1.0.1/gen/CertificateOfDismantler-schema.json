      "urn_bamm_io.catenax.shared.address_characteristic_1.0.1_PremiseEntity" : {
        "description" : "Characteristic for defining a premise which consists of a technical key (e.g. \"BUILDING\" or \"OTHER\") and a value (e.g. \"Werk 1\").",
