@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix :<urn:samm:io.catenax.binary_exchange:1.0.0#> .

:BinaryExchange a samm:Aspect ;
   samm:preferredName "binary exchange"@en ;
   samm:description "Metadata for exchanging a binary file. Access of the binary file via the dataspace protocol (DSP) is supported."@en ;
   samm:properties ( :mappingRuleId :fileContentDescriptionId :binaryFile [ samm:property :dspInformation; samm:optional true ] ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:mappingRuleId a samm:Property ;
   samm:preferredName "mapping rule id"@en ;
   samm:description "Unique identifier of the document specifying the mapping rules used for creating and reading the binary file."@en ;
   samm:characteristic samm-c:Text ;
   samm:see <https://catenax-ev.github.io/docs/standards/CX-0123-QualityUseCaseStandard#213-additional-requirements> ;
   samm:exampleValue "CX-0123" .

:fileContentDescriptionId a samm:Property ;
   samm:preferredName "file content description ID"@en ;
   samm:description "Unique identifier of the semantic model describing the content of the binary file."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "urn:samm:io.catenax.fleet.diagnostic_data:3.0.0" .

:binaryFile a samm:Property ;
   samm:preferredName "binary file"@en ;
   samm:description "Binary File to be exchanged."@en ;
   samm:characteristic :FileWithContentType .

:dspInformation a samm:Property ;
   samm:preferredName "dsp information"@en ;
   samm:description "DSP information."@en ;
   samm:characteristic :DspInformationCharacteristic .

:FileWithContentType a samm:Characteristic ;
   samm:preferredName "file with content type"@en ;
   samm:description "Characteristic for handling binary file."@en ;
   samm:dataType :ResourceWithContentType .

:DspInformationCharacteristic a samm:Characteristic ;
   samm:preferredName "DSP information characteristic"@en ;
   samm:description "DSP Information Characteristic"@en ;
   samm:dataType :DspInformationEntity .

:ResourceWithContentType a samm:Entity ;
   samm:preferredName "file with content type"@en ;
   samm:description "A file is a data element that represents an address to a file (a locator). The value is a URI that can represent an absolute or relative path."@en ;
   samm:see <https://admin-shell.io/aas/3/1/File> ;
   samm:properties ( [ samm:property :resourceValue; samm:payloadName "value" ] :contentType ) .

:DspInformationEntity a samm:Entity ;
   samm:preferredName "DSP information entity"@en ;
   samm:description "DSP Information Entity to group DSP information."@en ;
   samm:properties ( :controlPlaneUrl :dspAssetId ) .

:resourceValue a samm:Property ;
   samm:preferredName "resource value"@en ;
   samm:description "Path and name of the file (with file extension).\\n\\nThe path can be absolute or relative."@en ;
   samm:characteristic samm-c:ResourcePath ;
   samm:exampleValue "https://edc.dataplane.url/api/public/urn:uuid:a377ff49-6bde-4215-8d38-b8f02c991a35"^^xsd:anyURI .

:contentType a samm:Property ;
   samm:preferredName "content type"@en ;
   samm:description "Content type of that file."@en ;
   samm:characteristic samm-c:MimeType ;
   samm:exampleValue "application/octet-stream" .

:controlPlaneUrl a samm:Property ;
   samm:preferredName "control plane URL"@en ;
   samm:description "Control plane URL for accessing the binary file."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "https://semantics-edc-provider-controlplane.catenax.net/api/v1/dsp" .

:dspAssetId a samm:Property ;
   samm:preferredName "dsp asset ID"@en ;
   samm:description "DSP asset ID of the binary file."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "d99cfe94-bfa0-4eba-9e31-d30c4f31831b" .
