

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.shared.referenceData:1.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:position a samm:Property ;
   samm:preferredName "Position"@en ;
   samm:description "Indicates a poistion"@en ;
   samm:characteristic :SpatialPostionCharacteristic .

:SpatialPostionCharacteristic a samm:Characteristic ;
   samm:preferredName "Spatial postion characteristic"@en ;
   samm:description "Represents a single position in space with optional z coordinate."@en ;
   samm:dataType :SpatialPositionEntity .

:SpatialPositionEntity a samm:Entity ;
   samm:preferredName "Spatial position entity"@en ;
   samm:description "Represents latitude, longitude and altitude information in the WGS84 geodetic reference datum"@en ;
   samm:properties ( :latitude :longitude :altitude ) .

:latitude a samm:Property ;
   samm:preferredName "Latitude"@en ;
   samm:description "latitude coordinate in space (WGS84)"@en ;
   samm:characteristic :Coordinate ;
   samm:exampleValue "9.1781"^^xsd:decimal .

:longitude a samm:Property ;
   samm:preferredName "Longitude"@en ;
   samm:description "longitude coordinate in space (WGS84)"@en ;
   samm:characteristic :Coordinate ;
   samm:exampleValue "48.80835"^^xsd:decimal .

:altitude a samm:Property ;
   samm:preferredName "Altitude"@en ;
   samm:description "Elevation above sea level zero"@en ;
   samm:characteristic :MetresAboveMeanSeaLevel ;
   samm:exampleValue "153"^^xsd:float .

:Coordinate a samm-c:Measurement ;
   samm:preferredName "Coordinate"@en ;
   samm:description "Representing the geographical coordinate"@en ;
   samm:dataType xsd:decimal ;
   samm-c:unit samm-u:degreeUnitOfAngle .

:MetresAboveMeanSeaLevel a samm-c:Measurement ;
   samm:preferredName "metres above mean sea level"@en ;
   samm:description "Signifies the vertical distance in reference to a historic mean sea level as a vertical datum"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:metre .

:Text a samm:Characteristic ;
   samm:preferredName "Text"@en ;
   samm:description "Describes a Property which contains plain text. This is intended exclusively for human readable strings, not for identifiers, measurement values, etc."@en ;
   samm:dataType xsd:string .

:ISO4217Regex a samm-c:RegularExpressionConstraint ;
   samm:preferredName "ISO 4217 Regex"@en ;
   samm:description "ISO 4217 Regex"@en ;
   samm:see <https://www.iso.org/iso-4217-currency-codes.html> ;
   samm:value "^[A-Z]{3}$" .

:CountrySubdivisonTrait a samm-c:Trait ;
   samm:preferredName "Country Subdivison Trait"@en ;
   samm:description "ISO 3166-2 — Codes for the representation of names of countries and their subdivisions"@en ;
   samm-c:baseCharacteristic :Text ;
   samm-c:constraint :ISO31662Regex .

:currency a samm:Property ;
   samm:preferredName "Currency"@en ;
   samm:description "Currency code according to ISO 4217"@en ;
   samm:see <https://www.iso.org/iso-4217-currency-codes.html> ;
   samm:characteristic :CurrencyTrait ;
   samm:exampleValue "EUR" .

:CurrencyTrait a samm-c:Trait ;
   samm-c:baseCharacteristic samm-c:Text ;
   samm-c:constraint :ISO4217Regex .

:ISO31661Alpha3Regex a samm-c:RegularExpressionConstraint ;
   samm:preferredName "ISO 3166-1 Alpha 3Regex"@en ;
   samm:see <https://www.iso.org/iso-3166-country-codes.html> ;
   samm:value "^[A-Z]{3}$" .

:CountryAlpha3Trait a samm-c:Trait ;
   samm:preferredName "Country Alpha 3 Trait "@en ;
   samm:description "3 letter country code"@en ;
   samm-c:baseCharacteristic :Text ;
   samm-c:constraint :ISO31661Alpha3Regex .

:country a samm:Property ;
   samm:preferredName "Country"@en ;
   samm:description "Alphabetic three-character (alpha-3) code representing a country name"@en ;
   samm:see <https://www.iso.org/iso-3166-country-codes.html> ;
   samm:characteristic :CountryAlpha3Trait ;
   samm:exampleValue "DEU" .

:ISO31662Regex a samm-c:RegularExpressionConstraint ;
   samm:preferredName "ISO 3166-2 Regex"@en ;
   samm:description "Codes for the representation of names of countries and their subdivisions"@en ;
   samm:see <https://www.iso.org/iso-3166-country-codes.html> ;
   samm:value "^[A-Z]{2}-[A-Z0-9]{1,3}$" .

:countrySubdivision a samm:Property ;
   samm:preferredName "Country Subdivision"@en ;
   samm:description "Codes for the representation of names of countries and their subdivisions"@en ;
   samm:see <https://www.iso.org/iso-3166-country-codes.html> ;
   samm:characteristic :CountrySubdivisonTrait ;
   samm:exampleValue "US-CA" .

