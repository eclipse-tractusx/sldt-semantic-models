@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.simulated_week_based_capacity_group:1.0.0#> .
@prefix ext-week_based_capacity_group: <urn:samm:io.catenax.week_based_capacity_group:3.0.1#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#> .

:SimulatedWeekBasedCapacityGroup a samm:Aspect ;
   samm:preferredName "Simulated Capacity Group"@en ;
   samm:description "An entity that can contain multiple WeekBasedCapacityGroup in addition to the evaluation of a simulation scenario, requested by the customer."@en ;
   samm:properties ( :simulationId :simulatedWeekBasedCapacityGroupId :simulationStatus :simulationReadyDate :simulationChangedAt :listOfWeekBasedCapacityGroup ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:simulationId a samm:Property ;
   samm:preferredName "Simulation ID"@en ;
   samm:description "The simulation ID uniquely identifies a simulation scenario. All aspects that belong to the same simulation scenario share a simulation ID"@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "0157ba42-d2a8-4e28-8565-7b07830c1110" .

:simulatedWeekBasedCapacityGroupId a samm:Property ;
   samm:preferredName "Simulated Capacity Group ID"@en ;
   samm:description "The simulated capacity group ID uniquely identifies the simulated capacity group within the business relationship between a supplier and its customer."@en ;
   samm:characteristic ext-uuid:UuidV4Trait ;
   samm:exampleValue "0157ba42-d2a8-4e28-8565-7b07830c1110" .

:simulationStatus a samm:Property ;
   samm:preferredName "Simulation Status"@en ;
   samm:description "The simulation status indicates whether the supplier will actively work on a simulation scenario (ACCEPTED) or not (REJECTED). In addition it can indicate the outcome of a simulation, as scenario could be implemented (SUCCESS) or scenario cannot be implemented (FAILURE). Finally it can acknowledge that the customer has aborted the simulation scenario (ABORTED)."@en ;
   samm:characteristic :SimulationStatusCharacteristic ;
   samm:exampleValue "ACCEPTED" .

:SimulationStatusCharacteristic a samm-c:Enumeration ;
   samm:preferredName "Simulation Status Characteristic"@en ;
   samm:description "Values for simulation status"@en ;
   samm:dataType xsd:string ;
   samm-c:values (  "ACCEPTED" "REJECTED" "ABORTED" "SUCCESS" "FAILURE") .

:simulationReadyDate a samm:Property ;
   samm:preferredName "Simulation Ready Date"@en ;
   samm:description "The simulation ready date allows the supplier to indicate to the customer the date until the simulation scenario will be evaluated."@en ;
   samm:characteristic samm-c:Timestamp ;
   samm:exampleValue "2018-02-03T00:00:00"^^xsd:dateTime .

:simulationChangedAt a samm:Property ;
   samm:preferredName "Simulation Changed At"@en ;
   samm:description "Point in time when the content (any property according to the data model) of the simulated capacity group was changed, at the supplier, either by a human user or an automated process."@en ;
   samm:characteristic samm-c:Timestamp ;
   samm:exampleValue "2018-02-03T00:00:00"^^xsd:dateTime .

:listOfWeekBasedCapacityGroup a samm:Property ;
   samm:preferredName "Capacity Group List"@en ;
   samm:description "List of capacity groups that allow the supplier to further detail the simulation scenario evaluation in addition to simulation status."@en ;
   samm:characteristic :ListOfWeekBasedCapacityGroupCollection .

:WeekBasedCapacityGroup a samm:Entity ;
   samm:preferredName "WeekBasedCapacityGroup"@en ;
   samm:description "An existing WeekBasedCapacityGroup, potentially modified, that can be used to further detail the evaluation of the simulation scenario."@en ;
   samm:properties ( 
      ext-week_based_capacity_group:capacityGroupId 
      ext-week_based_capacity_group:name
      ext-week_based_capacity_group:supplierLocations
      ext-week_based_capacity_group:supplier
      ext-week_based_capacity_group:customer
      ext-week_based_capacity_group:unitOfMeasure
      ext-week_based_capacity_group:linkedDemandSeries
      ext-week_based_capacity_group:capacities
      ext-week_based_capacity_group:changedAt
      ext-week_based_capacity_group:linkedCapacityGroups 
      ext-week_based_capacity_group:unitOfMeasureIsOmitted 
      ext-week_based_capacity_group:demandVolatilityParameters 
      ext-week_based_capacity_group:capacityGroupIsInactive 
       ) .

:ListOfWeekBasedCapacityGroupCollection a samm-c:Collection ;
   samm:preferredName "Capacity Group Collection"@en ;
   samm:description "If a supplier wants to he can embed any number of existing and modified WeekBasedCapacityGroup with his simulated capacity group."@en ;
   samm:dataType :WeekBasedCapacityGroup .
