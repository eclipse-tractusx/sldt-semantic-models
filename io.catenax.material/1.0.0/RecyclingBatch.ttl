

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:catenax.io.material:1.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:3.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:RecyclingBatch a samm:Aspect ;
   samm:preferredName "RecyclingBatch"@en ;
   samm:description "Description of a recycling batch.\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n"@en ;
   samm:properties ( :recyclingBatchId :container ext-uuid:uuidV4Property ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:recyclingBatchId a samm:Property ;
   samm:preferredName "recycling batch Id"@en ;
   samm:description "Identifier of the recycling batch."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "BATCH-0001-9999-abcd" .

:container a samm:Property ;
   samm:preferredName "container"@en ;
   samm:description "Properties of a container."@en ;
   samm:characteristic :ContainerCharacteristic .

:ContainerCharacteristic a samm:Characteristic ;
   samm:preferredName "container Characteristic"@en ;
   samm:description "Container characteristics"@en ;
   samm:dataType :ContainerEntity .

:ContainerEntity a samm:Entity ;
   samm:preferredName "container entity"@en ;
   samm:description "Description of the properties of a container."@en ;
   samm:properties ( :containerWeight :containerVolume :containerDescription :containerTypeId ) .

:containerWeight a samm:Property ;
   samm:preferredName "container weight"@en ;
   samm:description "Empty weight of the container based on the scale display"@en ;
   samm:characteristic :ContainerWeightCharacteristic ;
   samm:exampleValue "3000.0"^^xsd:float .

:containerVolume a samm:Property ;
   samm:preferredName "container volume"@en ;
   samm:description "Volume of the container type according to AVAL"@en ;
   samm:characteristic :VolumeCharacteristic ;
   samm:exampleValue "20000"^^xsd:positiveInteger .

:containerDescription a samm:Property ;
   samm:preferredName "container description"@en ;
   samm:description "Standardized description of the container type according to AVAL"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Abrollcontainer (ARC)" .

:containerTypeId a samm:Property ;
   samm:preferredName "container type Id"@en ;
   samm:description "Standardized number for the container type which contains compositions / components / materials according to AVAL"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "AB.1.16.20000.A6B3C4D0E0F0F0H1I0J0K0L1.AR20-234" .

:ContainerWeightCharacteristic a samm-c:Measurement ;
   samm:preferredName "container weight characteristic"@en ;
   samm:dataType xsd:float ;
   samm-c:unit samm-u:kilogram .

:VolumeCharacteristic a samm-c:Measurement ;
   samm:preferredName "volume characteristic"@en ;
   samm:dataType xsd:positiveInteger ;
   samm-c:unit samm-u:litre .

