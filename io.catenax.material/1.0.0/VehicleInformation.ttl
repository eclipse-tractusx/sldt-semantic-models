

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:io.catenax.material:1.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:2.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:VehicleInformation a samm:Aspect ;
   samm:preferredName "vehicle information"@en ;
   samm:description "Contains all relevant vehicle information for the recycling of the vehicle."@en ;
   samm:properties ( ext-uuid:uuidV4Property :vehicleCondition :driveType :modelIdentifier :vehicleSeries :oem :emptyWeight :productionDate :vehicleType :anonymizedVin :wasteCode ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:vehicleCondition a samm:Property ;
   samm:preferredName "vehicle condition"@en ;
   samm:description "Condition of the actual vehicle."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "damaged" .

:driveType a samm:Property ;
   samm:preferredName "drive type"@en ;
   samm:description "Drive type of a vehicle according enumeration."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "combustion engine" .

:modelIdentifier a samm:Property ;
   samm:preferredName "model identifier"@en ;
   samm:description "OEM-specific model identifier or OEM-specific project name."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "E46" .

:vehicleSeries a samm:Property ;
   samm:preferredName "vehicle series "@en ;
   samm:description "Series information of the vehicle model that needs to be processed"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "3-Series" .

:oem a samm:Property ;
   samm:preferredName "Original equipment manufacturer"@en ;
   samm:description "Original equipment manufacturer"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Mercedes" .

:emptyWeight a samm:Property ;
   samm:preferredName "empty weight"@en ;
   samm:description "The empty weight of the vehicle in kg as specified."@en ;
   samm:characteristic :MassCharacteristic ;
   samm:exampleValue "1850.5"^^xsd:float .

:productionDate a samm:Property ;
   samm:preferredName "production date"@en ;
   samm:description "Production date of the vehicle "@en ;
   samm:characteristic :Date ;
   samm:exampleValue "2025-01-01"^^xsd:date .

:vehicleType a samm:Property ;
   samm:preferredName "vehicle type"@en ;
   samm:description "A vehicle type is a classification of a vehicle based on its design, construction, and purpose."@en ;
   samm:characteristic :VehicleTypeEnum .

:anonymizedVin a samm:Property ;
   samm:preferredName "anonymized VIN "@en ;
   samm:description "OEM-specific hashed VIN"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "WVWZZZ1JZXW000001" .

:Date a samm:Characteristic ;
   samm:preferredName "date"@en ;
   samm:description "date characteristic"@en ;
   samm:dataType xsd:date .

:VehicleTypeEnum a samm-c:Enumeration ;
   samm:preferredName "Vehicle Type Enum"@en ;
   samm:description "Enumeration of vehicle types according to EG Vehicle Classification"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "Electric and Hybrid Vehicles (Other than passenger cars)" "Special Purpose Vehicles" "Off-Road Vehicles / SUVs" "Motorcycles and Scooters" "Buses and Coaches" "Heavy Trucks / Lorries" "Light Commercial Vehicles (LCVs)" "Passenger Cars – Hybrid" "Passenger Cars – Electric Vehicle (EV)" "Passenger Cars – Internal Combustion Engine (ICE)" ) .

