

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:catenax.io.material:1.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:3.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:WasteCode a samm:Aspect ;
   samm:preferredName "waste code"@en ;
   samm:description "Describes the waste code of a part or material."@en ;
   samm:see <https://www.eea.europa.eu/help/glossary/eea-glossary/european-waste-catalogue-1> ;
   samm:properties ( :wasteCategorization ext-uuid:uuidV4Property ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:wasteCategorization a samm:Property ;
   samm:preferredName "waste categorization"@en ;
   samm:description "waste categorization"@en ;
   samm:characteristic :WasteCodeCharacteristic .

:WasteCodeCharacteristic a samm-c:Enumeration ;
   samm:preferredName "waste code characteristic"@en ;
   samm:description "waste code characteristic"@en ;
   samm:dataType :WasteCodeEntity ;
   samm-c:values ( :EndOfLifeVehicles1 :WastesNotOtherwiseSpecified :ComponentsNotOtherwiseSpecified :OtherHazardousComponents :TanksLiquefiedGas :OtherAntifreezeFluids :HazardousAntifreezeFluids :BrakeFluids :OtherBrakePads :BrakePads :ExplosiveComponents :ComponentsPCBs :ComponentsMercury :OilFilters :EndOfLifeVehicles2 :EndOfLifeTyres ) .

:WasteCodeEntity a samm:Entity ;
   samm:preferredName "waste code entity"@en ;
   samm:properties ( :wasteCodeDescription :wasteCode ) .

:EndOfLifeVehicles1 a :WasteCodeEntity ;
   :wasteCode "16 01 04" ;
   :wasteCodeDescription "end-of-life vehicles" .

:WastesNotOtherwiseSpecified a :WasteCodeEntity ;
   :wasteCode "16 01 99" ;
   :wasteCodeDescription "wastes not otherwise specified" .

:ComponentsNotOtherwiseSpecified a :WasteCodeEntity ;
   :wasteCode "16 01 22" ;
   :wasteCodeDescription "components not otherwise specified" .

:OtherHazardousComponents a :WasteCodeEntity ;
   :wasteCode "16 01 21" ;
   :wasteCodeDescription "hazardous components other than those mentioned in 16 01 07 to 16 01 11 and 16 01 13 and 16 01 14" .

:TanksLiquefiedGas a :WasteCodeEntity ;
   :wasteCode "16 01 16" ;
   :wasteCodeDescription "tanks for liquefied gas" .

:OtherAntifreezeFluids a :WasteCodeEntity ;
   :wasteCode "16 01 15" ;
   :wasteCodeDescription "antifreeze fluids other than those mentioned in 16 01 14" .

:HazardousAntifreezeFluids a :WasteCodeEntity ;
   :wasteCode "16 01 14" ;
   :wasteCodeDescription "antifreeze fluids containing hazardous substances" .

:BrakeFluids a :WasteCodeEntity ;
   :wasteCode "16 01 13" ;
   :wasteCodeDescription "brake fluids" .

:OtherBrakePads a :WasteCodeEntity ;
   :wasteCode "16 01 12" ;
   :wasteCodeDescription "brake pads other than those mentioned in 16 01 11" .

:BrakePads a :WasteCodeEntity ;
   :wasteCode "16 01 11" ;
   :wasteCodeDescription "brake pads containing asbestos" .

:ExplosiveComponents a :WasteCodeEntity ;
   :wasteCode "16 01 10" ;
   :wasteCodeDescription "explosive components (for example air bags)" .

:ComponentsPCBs a :WasteCodeEntity ;
   :wasteCode "16 01 09" ;
   :wasteCodeDescription "components containing PCBs" .

:ComponentsMercury a :WasteCodeEntity ;
   :wasteCode "16 01 08" ;
   :wasteCodeDescription "components containing mercury" .

:OilFilters a :WasteCodeEntity ;
   :wasteCode "16 01 07" ;
   :wasteCodeDescription "oil filters" .

:EndOfLifeVehicles2 a :WasteCodeEntity ;
   :wasteCode "16 01 06" ;
   :wasteCodeDescription "end-of-life vehicles, containing neither liquids nor other hazardous components" .

:EndOfLifeTyres a :WasteCodeEntity ;
   :wasteCode "16 01 03" ;
   :wasteCodeDescription "end-of-life tyres" .

:wasteCodeDescription a samm:Property ;
   samm:preferredName "waste code description"@en ;
   samm:description "Description of the related waste code. "@en ;
   samm:characteristic samm-c:Text .

:wasteCode a samm:Property ;
   samm:preferredName "waste code"@en ;
   samm:description "The waste code is used to identify the type of waste on a part or material level in accordance to EWC (european waste directory)."@en ;
   samm:characteristic samm-c:Text .

