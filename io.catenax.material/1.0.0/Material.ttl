

@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.1.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.1.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.1.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <urn:samm:catenax.io.material:1.0.0#> .
@prefix ext-classification: <urn:samm:catenax.io.material.classification:1.0.0#> .
@prefix ext-uuid: <urn:samm:io.catenax.shared.uuid:3.0.0#> .
@prefix samm-u: <urn:samm:org.eclipse.esmf.samm:unit:2.1.0#> .

:Material a samm:Aspect ;
   samm:preferredName "material"@en ;
   samm:properties ( :thermalCharacterization :chemicalCharacterization :physicalState :materialFormat :materialStatus :rangeOfUncertainty ext-uuid:uuidV4Property ext-classification:materialClassification ) ;
   samm:operations ( ) ;
   samm:events ( ) .

:thermalCharacterization a samm:Property ;
   samm:preferredName "thermal characterization"@en ;
   samm:description "Characterisation of Materials Thermal: Information about the material's behaviour under various thermal conditions."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Burning point" .

:chemicalCharacterization a samm:Property ;
   samm:preferredName "chemical characterization"@en ;
   samm:description "Characterisation of Materials Chemical: Description of the chemical composition and properties of the material.\n"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "LiOH" .

:physicalState a samm:Property ;
   samm:preferredName "physical state"@en ;
   samm:description "The physical state of the material."@en ;
   samm:characteristic :PhysicalStateEnum ;
   samm:exampleValue "liquid" .

:materialFormat a samm:Property ;
   samm:preferredName "material format"@en ;
   samm:description "Format of material in the process step."@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "Crushed Material / Flakes" .

:materialStatus a samm:Property ;
   samm:preferredName "material status"@en ;
   samm:description "Status of the material in the process step"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "shredded" .

:rangeOfUncertainty a samm:Property ;
   samm:preferredName "range of uncertainty"@en ;
   samm:description "Tolerance specification in material composition"@en ;
   samm:characteristic samm-c:Text ;
   samm:exampleValue "+/-10%" .

:PhysicalStateEnum a samm-c:Enumeration ;
   samm:preferredName "physical state enum"@en ;
   samm:description "physical state enum"@en ;
   samm:dataType xsd:string ;
   samm-c:values ( "solid" "liquid" "gas" "plasma" ) .

